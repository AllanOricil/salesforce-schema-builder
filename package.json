{
	"name": "vscode-extension-webview-template",
	"displayName": "vscode-extension-webview-template",
	"description": "A template of vscode webview extension.",
	"version": "0.0.1",
	"publisher": "leocll",
	"icon": "media/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/leocll/Beosin-VaaS-Extension.git"
	},
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:example.webview",
		"onCommand:example.helloWorld"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "example.webview",
				"title": "Example: WebView"
			},
			{
				"command": "example.helloWorld",
				"title": "Example: Hello World"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test",
		"build": "npm run buildweb && vsce package",
		"buildext": "vsce package",
		"buildweb": "pushd ./web &> /dev/null && { npm run build || popd &> /dev/null;} && popd &> /dev/null",
		"startweb": "pushd ./web &> /dev/null && { npm run dev || popd &> /dev/null;} && popd &> /dev/null"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
